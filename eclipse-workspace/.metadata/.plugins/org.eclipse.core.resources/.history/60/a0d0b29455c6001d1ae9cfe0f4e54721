
public class KoreanChar {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
	int i = 0;
	do {
	      char ch = (char)(i + 0xAC00);
	      System.out.printf("%c\t", ch);
	      if(ch == 0xA4B)
	    	  System.out.printf("\n=================================\n");
	      i++;
	}while(i != 11173);
		}

}
